{
  "name": "n8n-automation-templates",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun ./index.ts",
    "dev": "bun run start --dev",
    "rsync": "bun ./src/import-automations.ts",
    "build": "Bun build ./index.ts ./src/openapi.ts --format esm --target node --outdir ./dist && find dist -type f -name \"*.js\" -exec sed -i '' 's/\\.ts/\\.js/g' {} + && mkdir ./dist/automation && cp -rf ./automation/* ./dist/automation"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/figlet": "^1.7.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@langchain/core": "^0.3.72",
    "@langchain/openai": "^0.6.9",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/qs": "^6.14.0",
    "@types/swagger-ui-dist": "^3.30.6",
    "axios": "^1.11.0",
    "figlet": "^1.9.1",
    "fs-extra": "^11.3.1",
    "lodash-es": "^4.17.21",
    "openai": "^5.16.0",
    "openapi-types": "^12.1.3",
    "openapi3-ts": "^4.5.0",
    "process": "^0.11.10",
    "qs": "^6.14.0",
    "swagger-ui-dist": "^5.29.0"
  },
  "scarfSettings": {
    "enabled": false
  }
}
