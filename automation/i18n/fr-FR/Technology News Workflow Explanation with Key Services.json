          "additional": {
            "metaData": [
              {
                "name": "source_url",
                "value": "={{$json[\"sourceUrl\"]}}"
              }
            ]
          }
        },
        "credentials": {
          "wordpressApi": {
            "id": "2",
            "name": "WordPress account"
          }
        },
        "typeVersion": 1
      },
      {
        "id": "7",
