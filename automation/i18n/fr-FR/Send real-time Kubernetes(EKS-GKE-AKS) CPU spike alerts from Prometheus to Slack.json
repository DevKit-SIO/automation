        },
        "typeVersion": 2.2,
        "alwaysOutputData": true
      },
      {
        "id": "83f68a95-089f-4ed9-8847-99c85def9f49",
        "name": "Format Batched Slack Message",
        "type": "n8n-nodes-base.code",
        "position": [
          680,
          -140
        ],
        "parameters": {
          "jsCode": "return items.map(item => {\n  const pods = item.json[\"pods\"];\n  const app = item.json[\"app\"];\n  const namespace = pods[0].namespace;\n\n  let msg = `:rotating_light: *High CPU Spike Alert*\\n*App:* ${app}\\n*Namespace:* ${namespace}\\n\\n`;\n\n  for (const p of pods) {\n    msg += `• Pod: \\`${p.pod}\\` – CPU: *${p.cpu.toFixed(2)}* cores\\n`;\n  }\n\n  return {\n    json: {\n      text: msg\n    }\n  };\n});\n"
        },
        "typeVersion": 2
      },
      {
        "id": "6c3e1da0-645e-4f36-880c-8fee9c019552",
        "name": "Format Single Pod Slack Message",
        "type": "n8n-nodes-base.code",
        "position": [
          680,
          60
        ],
        "parameters": {
          "jsCode": "return items.map(item => {\n  const pods = item.json[\"pods\"];\n  const app = item.json[\"app\"];\n  const namespace = pods[0].namespace;\n\n  let msg = `:rotating_light: *High CPU Spike Alert*\\n*App:* ${app}\\n*Namespace:* ${namespace}\\n\\n`;\n\n  for (const p of pods) {\n    msg += `• Pod: \\`${p.pod}\\` – CPU: *${p.cpu.toFixed(2)}* cores\\n`;\n  }\n\n  return {\n    json: {\n      text: msg\n    }\n  };\n});\n"
        },
        "typeVersion": 2
      },
