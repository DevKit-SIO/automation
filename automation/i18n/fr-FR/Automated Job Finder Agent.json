        "parameters": {
          "jsCode": "\nconst raw = items[0].json;             \n\nconst output = raw.map(obj => ({\n  json: {\n \n    markdown: obj.markdown,\n\n  }\n}));\n\n\nreturn output;\n"
        },
        "typeVersion": 2
