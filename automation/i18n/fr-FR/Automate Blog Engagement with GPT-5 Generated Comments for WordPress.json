            "mode": "list",
            "value": "gpt-5",
            "cachedResultName": "GPT-5"
          },
          "options": {},
          "messages": {
            "values": [
              {
                "content": "=={{$json.prompt}}\n"
              }
            ]
          }
        },
        "credentials": {
          "openAiApi": {
            "id": "oETjE0hiLJ8SZxWX",
            "name": "YOUR_API_KEY"
          }
        },
        "retryOnFail": true,
        "typeVersion": 1.8,
        "waitBetweenTries": 2000
      },
      {
        "id": "4128f339-860f-4920-ae79-beb6c9bf43d4",
