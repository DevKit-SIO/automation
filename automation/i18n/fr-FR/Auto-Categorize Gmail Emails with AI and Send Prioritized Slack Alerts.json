        ],
        "parameters": {
          "text": "=Priority: {{$json.output.priority}}\nFrom: {{ $json.output.from }}\nSummary: {{ $json.output.summary }}\nReason: {{ $json.output.reason }}",
          "user": {
            "value": "={{ $json.slackChannel }}"
          },
          "select": "user",
          "otherOptions": {
            "includeLinkToWorkflow": false
          },
          "authentication": "oAuth2"
        },
        "typeVersion": 2.3
      }
