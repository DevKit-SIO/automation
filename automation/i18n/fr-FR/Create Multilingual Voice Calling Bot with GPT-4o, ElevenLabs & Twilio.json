              "caller_id": "={{ $json.From }}",
              "timestamp": "={{ $now }}",
              "request_details": "={{ $json.SpeechResult }}"
            },
            "mappingMode": "defineBelow"
          },
          "operation": "append",
          "sheetName": "Appointments",
          "documentId": "your-appointments-sheet-id"
        },
        "credentials": {
          "googleSheetsOAuth2Api": {
            "id": "google-sheets-creds",
            "name": "Google Sheets"
          }
        },
        "typeVersion": 3
      }
