      {
        "id": "63226aa9-0d9d-499d-988b-6b10b877fe7d",
        "name": "Format Alert Email",
        "type": "n8n-nodes-base.code",
        "position": [
          832,
          0
        ],
        "parameters": {
          "jsCode": "const s=(n,d=2)=>isFinite(n)?Number(n).toFixed(d):'‚Äî';\nconst sign=n=>isFinite(n)&&n>0?'+':'';\nconst j=$json; const lines=[\n  `BTC: $${s(j.btc.price,0)} (24h: ${sign(j.btc.chg24)}${s(j.btc.chg24)}%)`,\n  `ETH: $${s(j.eth.price,0)} (24h: ${sign(j.eth.chg24)}${s(j.eth.chg24)}%)`\n];\nreturn [{json:{\n  subject:`‚ö†Ô∏è Crypto Alert ‚Äî ${new Date().toLocaleString('en-US',{timeZone:'Africa/Lagos'})}`,\n  html:`<p>${lines.join('<br>')}</p>`,\n  text: lines.join('\\n')\n}}];"
        },
        "typeVersion": 2
      },
