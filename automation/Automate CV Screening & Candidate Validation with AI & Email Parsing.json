    "nodes": [
      {
        "id": "2c5dae24-563c-4e86-958f-b267b80c7b97",
        "name": "Trigger on New CV Email\t",
        "type": "n8n-nodes-base.emailReadImap",
        "position": [
          -880,
          60
        ],
        "parameters": {
          "format": "resolved",
          "options": {
