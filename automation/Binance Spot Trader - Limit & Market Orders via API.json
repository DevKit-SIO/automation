        "parameters": {
          "jsCode": "// ‡πÅ‡∏õ‡∏•‡∏á JSON object ‡πÄ‡∏õ‡πá‡∏ô query string\nconst data = items[0].json;\n\n// ‡∏ß‡∏¥‡∏ò‡∏µ‡∏ó‡∏µ‡πà 1: ‡πÉ‡∏ä‡πâ Object.keys() ‡πÅ‡∏•‡∏∞ map()\nconst queryString = Object.keys(data)\n  .map(key => `${key}=${data[key]}`)\n  .join('&');\n\nreturn [{\n  json: {\n    queryString: queryString,\n    originalData: data\n  }\n}];"
        },
        "typeVersion": 2
      },
